local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Charmed | Noah*#750", "Ocean")

-- Unlocker
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewButton("Temporal Status", "Z", function()
     game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "z" then
    local args = {
        [1] = "Temporal"
    }
    
    game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))
    
end
end)
end)


MainSection:NewButton("Piper Combust", "F", function()
    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
        if key == "f" then
                             local args = {
           [1] = "Molecular Combustion"
       }
       
       game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))
       
        end
       end)
end)


MainSection:NewButton("Area Push", "R", function()
     game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "r" then
local args = {
    [1] = "Telekinetic Area Push"
}

game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))

end
end)
end)


